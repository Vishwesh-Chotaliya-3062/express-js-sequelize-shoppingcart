<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0 ,user-scalable=no">
    <link rel="shortcut icon" href="/images/favicon.ico">
    <title>Status</title>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/responsive.css">
</head>

<body>

    <section id="nav-header" style="height: 100px;">
        <img src="images/pre-login/banner-with-overlay.jpg" id="login-bg-img" alt="">

        <header>
            <nav class="navbar navbar-expand-md fixed-top white-nav">
                <div class="container">
                    <div class="img-logo">
                            <img src="/images/logo.png" class="img-fluid" alt="">
                    </div>

                    <button class="navbar-toggler" type="button" data-toggle="collapse"
                        data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" style="margin-left: auto; float: right;"
                        id="navbarSupportedContent">
                        <ul class="navbar-nav" style="margin-left: auto;">
                            <% for(let user in userDetails) { %>
                                <li class="nav-item">
                                    <a class="nav-link purple-link">Welcome <b style="color: #6255a6;">&nbsp;<%=
                                                userDetails[user].UserName %>!</b></a>
                                </li>
                                <% } %>
                                    <li class="nav-item">
                                        <a class="nav-link purple-link" href="/welcome">HOME</a>
                                    </li>

                                    <li class="nav-item">
                                        <a class="nav-link purple-link" href="/wallet">WALLET( $<b style="color: #6255a6;"><%= walletBalance %>
                                            </b>)</a>
                                    </li>

                                    <% if(!cartCount) { %>
                                        <li class="nav-item">
                                            <a class="nav-link purple-link" href="/cart">CART</a>
                                        </li>

                                        <% } else{ %>
                                            <li class="nav-item">
                                                <a class="nav-link purple-link" href="/cart">CART(<b
                                                        style="color: #6255a6;">
                                                        <%= cartCount %>
                                                    </b>)</a>
                                            </li>
                                            <% } %>

                                                <% if(!countCouponcode) { %>
                                                    <li class="nav-item">
                                                        <a class="nav-link purple-link" href="/couponcode">COUPON
                                                            CODE</a>
                                                    </li>

                                                    <% } else{ %>
                                                        <li class="nav-item">
                                                            <a class="nav-link purple-link" href="/couponcode">COUPON
                                                                CODE (<b style="color: #6255a6;">
                                                                    <%= countCouponcode %>
                                                                </b>)</a>
                                                        </li>
                                                        <% } %>

                                                            <% for(let user in userDetails) { %>
                                                                <% if(userDetails[user].Status=='pending' ) { %>
                                                                    <li class="nav-item">
                                                                        <a id="nav-btn" style="width: 150px;"
                                                                            class="nav-link btn btn-login-navbar-purple white-link d-flex align-items-center"
                                                                            href="<%= link %>" role="button">VERIFY
                                                                            EMAIL</a>
                                                                    </li>
                                                                    <% } %>
                                                                        <% } %>
                                                                            <li class="nav-item">
                                                                                <a id="nav-btn"
                                                                                    class="nav-link btn btn-login-navbar-purple white-link d-flex align-items-center"
                                                                                    href="/logout">LOGOUT</a>
                                                                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
    </section>

    <section id="login">

        <div id="login-content" style="margin-top: 50px;">
            <div id="logo">
                <img src="images/pre-login/top-logo.png" class="img-fluid" alt="" style="margin-top: -20px;">
            </div>

            <% if(Data1.Status == "success") { %>

            <div class="container-cust" id="login-form" style="margin-bottom: 0px; margin-top: -30px;">

                <div style="margin-left: 260px; margin-top: 30px;">
                    <img src="/images/SUCCESS.png" style="height: 60px; width: 60px;">
                </div>
                <div id="login-heading" style="margin-top:-40px;">
                    <p>THANK YOU FOR PURCHASE</p>
                </div>
               
                <div style="margin-left: 140px;">
                    <div class="d-flex align-items-center">
                        <div class="display-5">Order Number </div>
                        <div class="display-5" style="margin-left: 67px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            #<%= Data1.id %>
                        </div>
                    </div>
                    <div class="d-flex align-items-center">
                        <div class="display-5">Subtotal </div>
                        <div class="display-5" style="margin-left: 112px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            $<%= Data1.TotalAmount %>
                        </div>
                    </div>
                    <div class="d-flex align-items-center">
                        <div class="display-5">Discount </div>
                        <div class="display-5" style="margin-left: 110px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            $<%= Data1.DiscountedAmount %>
                        </div>
                    </div>
                    <div class="d-flex align-items-center">
                        <div class="display-5">Purchase Total </div>
                        <div class="display-5" style="margin-left: 72px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            $<%= Data1.PurchaseTotal %>
                        </div>
                    </div>
                    <div class="d-flex align-items-center">
                        <div class="display-5">Order Date </div>
                        <div class="display-5" style="margin-left: 72px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            $<%= Data1.Date %>
                        </div>
                    </div>
                </div>
            </div>

            <% } else if(Data1.Status == "failed") { %>

            <div class="container-cust" id="login-form" style="margin-bottom: 0px; margin-top: -30px;">
                <div style="margin-left: 260px; margin-top: 30px;">
                    <img src="/images/FAILED.png" style="height: 60px; width: 60px;">
                </div>
                <div id="login-heading" style="margin-top: -40px;">
                    <p><%= outofstock %> Out of Stock</p>                    
                </div>
                <div>
                <div style="margin-left: 140px;">
                    <div class="d-flex align-items-center">
                        <div class="display-5">Order Number </div>
                        <div class="display-5" style="margin-left: 67px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            #<%= Data1.id %>
                        </div>
                    </div>
                    <div class="d-flex align-items-center">
                        <div class="display-5">Subtotal </div>
                        <div class="display-5" style="margin-left: 112px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            $<%= Data1.TotalAmount %>
                        </div>
                    </div>
                    <div class="d-flex align-items-center">
                        <div class="display-5">Discount </div>
                        <div class="display-5" style="margin-left: 110px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            $<%= Data1.DiscountedAmount %>
                        </div>
                    </div>
                    <div class="d-flex align-items-center">
                        <div class="display-5">Purchase Total </div>
                        <div class="display-5" style="margin-left: 72px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            $<%= Data1.PurchaseTotal %>
                        </div>
                    </div>
                    <div class="d-flex align-items-center">
                        <div class="display-5">Order Date </div>
                        <div class="display-5" style="margin-left: 72px;">:</div>
                        <div class="ml-auto font-weight-bold" style="margin-right: 140px;">
                            $<%= Data1.Date %>
                        </div>
                    </div>
                </div>
            </div>
            <% } %>
        </div>

    </section>

    <script src="js/jquery-3.5.1.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/script.js"></script>

</body>